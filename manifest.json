{
  "manifest_version": 2,
  "name": "Auto Clicker",
  "version": "1.0",
  "description": "Automatically clicks buttons on the page",
  "permissions": ["activeTab"],
  "browser_action": {
    "default_title": "Start Auto Clicker"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}